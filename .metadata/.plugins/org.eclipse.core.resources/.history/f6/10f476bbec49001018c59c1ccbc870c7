
#include "stm32l4xx.h"
#include "fpu.h"
#include "uart.h"
#include <stdio.h>
#include "timebase.h"
#include "bsp.h"


int main(void)
{
	/* Enable FPU */
	FPU_Enable();

	/* Initialize UART */
	Debug_UART_Init();

	/* Initialize Timebase */
	Timebase_Init();

	/* Initialize LED */
	LED_Init();

	/* Initialize button */

	while(1)
	{
		printf("Hello from STM32\r\n");
		Delay(1);
	}
}
