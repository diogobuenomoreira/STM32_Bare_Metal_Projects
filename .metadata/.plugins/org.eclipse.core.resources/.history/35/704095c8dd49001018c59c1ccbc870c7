#include "stm32l4xx.h"
#include "bsp.h"

#define GPIOAEN		(1U << 0U)
#define PIN5		(1U<<5U)
#define LED_PIN		PIN5

void LED_Init(void)
{
	/* Enable clock access to GPIOA */
	RCC->AHB2ENR |= GPIOAEN;

	/* Set PA5 mode to output mode */

}
